  width: 90%;
  padding: 20rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: #e9e8e6;
  border-radius: 20rpx; 
  
}

.view-hearder {
  display: flex;
  flex-direction: row;
  width: 100%;
}

.hearder-name-view {
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
}

.one-view {
  font-size: 30rpx;
